<div class="parallax" id="portfolio">
  <h1 class="upcased text-center white-text shadow-text">Portfolio</h1>
</div>
<div class="container padding center-block">
  <div class="row">
    <% @portfolio.each do |image| %>
      <div class="col-sm-6 col-md-6 col-lg-4">
        <div class="thumbnail">
          <img src="<%= image[:url]%>">
          <div class="caption text-center">
            <h4><%= image[:title] %></h4>
            <p><%= image[:description] %></p>
            <div class="source-icon">
            <p><a href="<%= image[:source]%>" class="green-text <%= image[:icon]%>"></a>
            </div>
              <!-- <a href="#" class="btn btn-default" role="button">Button</a> -->
            </p>
          </div>
        </div>
      </div>
    <% end %>
  </div>

</div>




